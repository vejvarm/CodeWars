    "def find_nb(m):\n",
    "    # probably not optimal but it works\n",
    "    sum_n = 0\n",
    "    n = 1\n",
    "\n",
    "    while sum_n < m:\n",
    "        sum_n += n**3\n",
    "        n += 1\n",
    "\n",
    "    print(sum_n)\n",
    "        \n",
    "    if sum_n == m:\n",
    "        return n-1\n",
    "    else:\n",
    "        return -1"
   ]
